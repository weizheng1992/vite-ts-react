{
  "name": "vite-react-ts-app",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "npm run install",
    "dev": "cross-env --max_old_space_size=4096 vite  --mode development --host",
    "test": "cross-env --max_old_space_size=4096 vite --mode test --host",
    "build": "vite build",
    "serve": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky"
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "@ant-design/icons": "^5.0.1",
    "@iconify/iconify": "^2.2.1",
    "@redux-saga/core": "^1.2.3",
    "antd": "^4.24.10",
    "axios": "^0.21.4",
    "history": "^5.3.0",
    "immer": "^9.0.21",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.29.4",
    "qs": "^6.11.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "react-use": "^17.4.0",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.8",
    "tinymce": "^5.10.7",
    "use-immer": "^0.6.0",
    "windicss": "^3.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@iconify/json": "^1.1.461",
    "@purge-icons/generated": "^0.7.0",
    "@types/http-proxy": "^1.17.11",
    "@types/lodash-es": "^4.17.7",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^14.18.44",
    "@types/react": "^17.0.58",
    "@types/react-dom": "^17.0.20",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^6.0.0",
    "less": "^4.1.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "prop-types": "^15.8.1",
    "rollup-plugin-visualizer": "5.5.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-compression": "^0.2.6",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-pwa": "^0.8.2",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-svg-icons": "^1.1.0",
    "vite-plugin-windicss": "^1.8.10"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.52.2"
  }
}
