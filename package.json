{
  "name": "vite-react-ts-app",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "cross-env --max_old_space_size=4096 vite  --mode development --host",
    "test": "cross-env --max_old_space_size=4096 vite --mode test --host",
    "build": "vite build",
    "serve": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky"
  },
  "dependencies": {
    "@iconify/iconify": "^2.0.4",
    "antd": "^4.16.10",
    "axios": "^0.21.1",
    "history": "^5.0.0",
    "immer": "^9.0.3",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "qs": "^6.10.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.0.0-beta.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "tinymce": "^5.8.1",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@iconify/json": "^1.1.398",
    "@purge-icons/generated": "^0.7.0",
    "@types/http-proxy": "^1.17.5",
    "@types/lodash-es": "^4.17.4",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-legacy": "^1.4.2",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.3.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "rollup-plugin-visualizer": "5.5.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "4.3.4",
    "vite": "2.3.8",
    "vite-plugin-compression": "^0.2.5",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-pwa": "^0.8.1",
    "vite-plugin-style-import": "^1.0.1",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-windicss": "^1.3.0"
  },
  "resolutions": {
    "//": "Used to install imagemin dependencies, because imagemin may not be installed in China. If it is abroad, you can delete it",
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.52.2"
  }
}
