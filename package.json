{
  "name": "vite-react-ts-app",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "cross-env --max_old_space_size=4096 vite --mode development",
    "test": "cross-env --max_old_space_size=4096 vite --mode test",
    "build": "vite build",
    "serve": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky"
  },
  "dependencies": {
    "antd": "^4.15.0",
    "axios": "^0.21.1",
    "history": "^5.0.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "qs": "^6.10.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/http-proxy": "^1.17.5",
    "@types/lodash-es": "^4.17.4",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/rollup-plugin-visualizer": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "rollup-plugin-visualizer": "^4.2.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.1.2",
    "vite": "^2.1.0",
    "vite-plugin-compress": "^2.1.0",
    "vite-plugin-compression": "^0.2.3",
    "vite-plugin-html": "^2.0.3",
    "vite-plugin-imp": "^2.0.5",
    "vite-plugin-pwa": "^0.6.4",
    "vite-plugin-style-import": "^0.10.0",
    "vite-plugin-windicss": "^0.11.1"
  }
}
